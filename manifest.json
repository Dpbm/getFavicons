{
  "manifest_version": 2,
  "name": "GetFavicons",
  "version": "1.0",
  "description": "Get favicons from websites",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_title": "Favicons list",
    "default_popup": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
